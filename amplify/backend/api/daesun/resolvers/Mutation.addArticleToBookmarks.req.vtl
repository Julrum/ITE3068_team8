{
    "version": "2017-02-28",
    "operation": "UpdateItem",
    "key": {
        "id": { "S": "$ctx.args.input.id"}
    },
    "update": {
        "expression":"SET bookmarkList = list_append(if_not_exists(bookmarkList, :emptyList), :newArticle)",
        "expressionValues":
          {
            ":newArticle":{"L": [{"S":"$ctx.args.input.article"}]},
            ":emptyList":{"L": []}
          }
        }
}